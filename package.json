{
  "name": "ide-livescript",
  "version": "0.0.0",
  "description": "LiveScript language support for Atom-IDE",
  "main": "lib/client.js",
  "enhancedScopes": [
    "source.livescript"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "scripts": {
    "develop": "./node_modules/.bin/lsc dev/watch.ls",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Bartosz Matuszewski <bartosz.wojciech.matuszewski@gmail.com>",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/bartosz-m/atom-ide-livescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/bartosz-m/atom-ide-livescript.git"
  },
  "dependencies": {
    "atom-languageclient": "^0.7.2",
    "fuzzysort": "^1.0.2",
    "livescript": "git+https://github.com/bartosz-m/LiveScript.git#macros",
    "livescript-compiler": "0.0.35",
    "livescript-transform-esm": "^2.12.0",
    "livescript-transform-implicit-async": "^1.0.7",
    "livescript-transform-object-create": "1.0.7",
    "livescript-transform-top-level-await": "0.0.5",
    "source-map": "^0.6.1",
    "vscode-languageserver": "^3.5.0"
  },
  "devDependencies": {
    "chokidar": "^1.7.0",
    "fs-extra": "^4.0.3"
  },
  "engines": {
    "atom": ">=1.19.7"
  },
  "private": false
}
