{
  "name": "ide-livescript",
  "version": "0.0.0",
  "main": "lib/client.js",
  "enhancedScopes": [
    "source.livescript"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "scripts": {
    "develop": "./node_modules/.bin/lsc dev/watch.ls",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "atom-languageclient": "^0.5.0",
    "vscode-languageserver": "^3.3.0"
  },
  "devDependencies": {
    "chokidar": "^1.7.0",
    "fs-extra": "^4.0.1",
    "livescript-async": "^1.5.2"
  },
  "engines": {
    "atom": ">=1.19.7"
  },
  "private": true
}
